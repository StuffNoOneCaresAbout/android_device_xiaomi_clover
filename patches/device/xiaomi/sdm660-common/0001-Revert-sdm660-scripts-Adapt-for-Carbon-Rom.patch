index 48d89394..e78be705 100755
--- a/setup-makefiles.sh
+++ b/setup-makefiles.sh
@@ -28,7 +28,7 @@ if [[ ! -d "$MY_DIR" ]]; then MY_DIR="$PWD"; fi
 
